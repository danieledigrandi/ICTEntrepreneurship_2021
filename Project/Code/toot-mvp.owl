@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix toot: <https://toot.ai/2021/04/MVP#> .

<https://toot.ai/2021/04/MVP>
  a owl:Ontology ;
  rdfs:comment "This ontology describes a hierarchy of service components commonly used by MSPs today"@en ,
  "Deze ontologie beschrijft een hiërarchie van servicecomponenten die tegenwoordig algemeen door MSP's worden gebruikt"@nl ;
  owl:versionIRI <https://toot.ai/2021/04/MVP> ;
  owl:versionInfo "$Date: 2021/04/08 13:37:00 $" ;
  rdfs:label "Service components"@en, "Service componenten"@nl .

toot:Acrylic
  a owl:Class ;
  rdfs:subClassOf toot:SyntheticFibre ;
  rdfs:label "Acrylic"@en, "Acrílico"@es, "Acrylique"@fr .

toot:BastFibre
  a owl:Class ;
  rdfs:subClassOf toot:ClothingMaterial ;
  rdfs:label "Bast fibre"@en, "Fibras del líber"@es, "Fibres libériennes"@fr .

toot:CellulosicFibre
  a owl:Class ;
